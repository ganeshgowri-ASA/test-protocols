        "icon": "\ud83d\udcca"
      },
      {
        "id": "analysis",
        "name": "Analysis",
        "icon": "\ud83d\udd2c"
      },
      {
        "id": "validation",
        "name": "Validation",
        "icon": "\u2705"
      },
      {
        "id": "report",
        "name": "Report",
        "icon": "\ud83d\udcc4"
      }
    ]
  }
