  "analysis_and_reporting": {
    "time_series_analysis": {
      "chamber_stability_assessment": {
        "metrics": [
          "Temperature stability over 8 hours",
          "Underpressure consistency",
          "Dust concentration uniformity (if measured)"
        ],
        "visualization": [
          "Temperature vs. time",
          "Underpressure vs. time",
          "Pressure control chart with ±2 Pa limits"
        ]
      }
    },
    "baseline_comparison": {
      "electrical_parameters": {
        "comparison_metrics": [
          {
            "parameter": "Pmax",
            "calculation": "Δ% = 100 × (Pmax_post - Pmax_pre) / Pmax_pre",
            "threshold": "< 1% (no degradation expected)",
            "criticality": "Failure indicator if significant degradation"
          },
          {
            "parameter": "Riso",
            "calculation": "Post-test value",
            "threshold": "≥ 40 MΩ",
            "criticality": "Critical - dust ingress can reduce insulation"
          }
        ],
        "visualization": [
          "Pre/post IV curve overlay",
          "Insulation resistance bar chart"
        ]
      },
      "visual_inspection": {
        "junction_box_comparison": "Before/after interior photographs",
        "dust_ingress_mapping": "Component-level ingress assessment",
        "ip6x_rating": "Pass / Fail per IEC 60529"
      }
    },
    "degradation_modeling": {
      "dust_ingress_pathways_analysis": {
        "investigation": "If dust ingress detected, identify pathway",
        "common_failure_modes": [
          "Junction box gasket compression set or damage",
          "Cable gland seal degradation",
          "Frame drainage hole design allows dust ingress",
          "Inadequate seal compression or misalignment"
        ],
        "corrective_action": "Design modification to improve sealing"
      },
      "field_correlation": {
        "approach": "IP6X test simulates worst-case dust exposure",
        "field_validation": "Modules passing IP6X suitable for desert installations",
