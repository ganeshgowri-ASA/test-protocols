{
  "protocol_metadata": {
    "protocol_id": "PVTP-032",
    "version": "1.0",
    "title": "Hail Impact Test (IEC 61215 MQT 19)",
    "description": "Mechanical impact resistance test using 25mm ice balls to simulate hail damage on PV modules",
    "test_type": "environmental_mechanical",
    "created_date": "2025-11-12",
    "last_modified": "2025-11-12",
    "author": "PV Testing Laboratory",
    "status": "active",
    "compliance_standards": [
      {
        "standard_id": "IEC 61215",
        "version": "4.0",
        "section": "MQT 19",
        "title": "Mechanical Load Test - Hail",
        "requirements": [
          "25mm diameter ice balls at 23 m/s impact velocity",
          "11 impact points per module",
          "No visual defects, <5% power degradation post-test"
        ]
      },
      {
        "standard_id": "IEC 61730",
        "version": "2.0",
        "section": "MST 23",
        "title": "Impact Test"
      }
    ],
    "related_protocols": ["PVTP-001", "PVTP-037", "PVTP-038"],
    "approval_status": {
      "reviewed_by": "Technical Manager",
      "approved_by": "Quality Director",
      "approval_date": "2025-11-12"
    }
  },
  "test_objectives": {
    "primary_objective": "Verify PV module resistance to hail impact damage per IEC 61215 MQT 19",
    "secondary_objectives": [
      "Assess structural integrity after repeated impacts",
      "Evaluate glass strength and lamination quality",
      "Measure power degradation post-impact",
      "Identify potential failure modes and weak points"
    ],
    "acceptance_criteria": {
      "visual_inspection": "No cracks, delamination, or junction box damage",
      "electrical_performance": "Power degradation ≤ 5% from baseline",
      "insulation_resistance": "≥ 40 MΩ (wet leakage current test)",
      "safety": "Pass dielectric withstand test at 1000V + (2 × Voc max)"
    }
  },
  "test_equipment": {
    "hail_impact_simulator": {
      "manufacturer": "Atlas Material Testing",
      "model": "HTS-45 Hail Test System",
      "specifications": {
        "ice_ball_diameter": {
          "value": 25,
          "unit": "mm",
          "tolerance": "±1 mm"
        },
        "impact_velocity": {
          "value": 23,
          "unit": "m/s",
          "tolerance": "±1 m/s"
        },
        "launch_mechanism": "Pneumatic cannon",
        "velocity_measurement": "Dual laser gate system",
        "impact_angle": {
          "value": 90,
          "unit": "degrees",
          "tolerance": "±5 degrees"
        }
      },
      "calibration": {
        "frequency": "quarterly",
        "last_calibration_date": "2025-08-15",
        "next_calibration_date": "2025-11-15",
        "calibration_certificate": "CAL-HTS-2025-Q3-001",
        "traceable_to": "NIST"
      }
    },
    "ice_production_system": {
      "manufacturer": "Custom Built",
      "type": "Ice ball mold system",
      "specifications": {
        "water_source": "Distilled water",
        "freezing_temperature": -20,
        "freezing_duration": "≥24 hours",
        "storage_temperature": -10
      }
    },
    "measurement_instruments": {
      "high_speed_camera": {
        "model": "Phantom VEO 640L",
        "frame_rate": "1000 fps",
        "purpose": "Impact documentation"
      },
      "iv_curve_tracer": {
        "model": "PVE PV Analyzer 1500V",
        "accuracy": "±0.5%",
        "purpose": "Pre/post electrical characterization"
      },
      "insulation_tester": {
        "model": "Fluke 1550C",
        "range": "250V - 5kV",
        "purpose": "Insulation resistance measurement"
      }
    },
    "environmental_conditions": {
      "ambient_temperature": {
        "target": 25,
        "unit": "°C",
        "tolerance": "±5°C"
      },
      "relative_humidity": {
        "target": 50,
        "unit": "%",
        "tolerance": "±20%"
      }
    }
  },
  "test_sequence": {
    "pre_test_phase": {
      "duration": "2-4 hours",
      "steps": [
        {
          "step_number": 1,
          "action": "Visual inspection and documentation",
          "parameters": {
            "inspection_areas": ["Front glass", "Back sheet", "Frame", "Junction box", "Cables"],
            "documentation": "High-resolution photographs (all areas)"
          },
          "acceptance": "No pre-existing damage"
        },
        {
          "step_number": 2,
          "action": "Electrical characterization (baseline)",
          "parameters": {
            "measurements": ["Voc", "Isc", "Pmax", "Vmp", "Imp", "FF"],
            "irradiance": "1000 W/m²",
            "module_temperature": "25°C",
            "spectrum": "AM 1.5G"
          },
          "data_logging": {
            "file_name": "baseline_iv_curve.csv",
            "parameters_logged": ["voltage", "current", "power", "temperature"]
          }
        },
        {
          "step_number": 3,
          "action": "Insulation resistance test",
          "parameters": {
            "test_voltage": "1000V DC",
            "duration": "60 seconds",
            "min_resistance": "40 MΩ"
          }
        },
        {
          "step_number": 4,
          "action": "Wet leakage current test",
          "parameters": {
            "surface_resistance": "≥ 1 MΩ",
            "test_voltage": "Calculated per IEC 61215"
          }
        },
        {
          "step_number": 5,
          "action": "Module mounting and positioning",
          "parameters": {
            "mounting_angle": "Horizontal (face up)",
            "support": "Rigid frame per IEC 61215",
            "clamping": "Standard mounting clamps"
          }
        }
      ]
    },
    "impact_test_phase": {
      "duration": "30-60 minutes",
      "steps": [
        {
          "step_number": 6,
          "action": "Ice ball preparation and conditioning",
          "parameters": {
            "ice_ball_diameter": "25 mm ±1mm",
            "quantity": "11 per module",
            "storage_temperature": "-10°C",
            "pre_impact_conditioning": "Remove from freezer 2 minutes before launch"
          }
        },
        {
          "step_number": 7,
          "action": "Impact velocity calibration",
          "parameters": {
            "target_velocity": "23 m/s",
            "tolerance": "±1 m/s",
            "verification": "Laser gate measurement for each shot",
            "pneumatic_pressure_adjustment": "As needed"
          }
        },
        {
          "step_number": 8,
          "action": "Execute impact sequence",
          "parameters": {
            "total_impacts": 11,
            "impact_pattern": "Per IEC 61215 Figure 16",
            "impact_locations": [
              {"location": "Center", "x": 0, "y": 0},
              {"location": "Cell center", "x": "+150mm", "y": 0},
              {"location": "Cell center", "x": "-150mm", "y": 0},
              {"location": "Cell center", "x": 0, "y": "+150mm"},
              {"location": "Cell center", "x": 0, "y": "-150mm"},
              {"location": "Cell edge", "x": "+300mm", "y": "+300mm"},
              {"location": "Cell edge", "x": "-300mm", "y": "+300mm"},
              {"location": "Cell edge", "x": "+300mm", "y": "-300mm"},
              {"location": "Cell edge", "x": "-300mm", "y": "-300mm"},
              {"location": "Corner bypass diode area", "x": "+400mm", "y": "+500mm"},
              {"location": "Corner junction box area", "x": "+400mm", "y": "-500mm"}
            ],
            "interval_between_impacts": "30 seconds minimum"
          },
          "data_logging_per_impact": {
            "parameters": [
              "impact_number",
              "timestamp",
              "ice_ball_diameter",
              "measured_velocity",
              "impact_location_x",
              "impact_location_y",
              "ambient_temperature",
              "relative_humidity"
            ],
            "video_recording": "High-speed (1000 fps) + standard video"
          }
        },
        {
          "step_number": 9,
          "action": "Post-impact visual inspection",
          "parameters": {
            "timing": "Immediately after each impact",
            "inspection": "Document any visible damage",
            "photography": "High-resolution images of impact points"
          }
        }
      ]
    },
    "post_test_phase": {
      "duration": "2-4 hours",
      "steps": [
        {
          "step_number": 10,
          "action": "Final visual inspection",
          "parameters": {
            "inspection_areas": ["All impact points", "Back sheet", "Frame", "Junction box", "Cables"],
            "documentation": "Comprehensive photographic record",
            "defect_classification": "Per IEC 61215 Section 7.1"
          }
        },
        {
          "step_number": 11,
          "action": "Electrical characterization (post-test)",
          "parameters": {
            "measurements": ["Voc", "Isc", "Pmax", "Vmp", "Imp", "FF"],
            "irradiance": "1000 W/m²",
            "module_temperature": "25°C",
            "spectrum": "AM 1.5G"
          },
          "data_logging": {
            "file_name": "post_test_iv_curve.csv",
            "parameters_logged": ["voltage", "current", "power", "temperature"]
          }
        },
        {
          "step_number": 12,
          "action": "Insulation resistance test",
          "parameters": {
            "test_voltage": "1000V DC",
            "duration": "60 seconds",
            "min_resistance": "40 MΩ"
          }
        },
        {
          "step_number": 13,
          "action": "Wet leakage current test",
          "parameters": {
            "surface_resistance": "≥ 1 MΩ",
            "test_voltage": "Calculated per IEC 61215"
          }
        },
        {
          "step_number": 14,
          "action": "Power degradation calculation",
          "parameters": {
            "formula": "Degradation (%) = 100 × (Pmax_baseline - Pmax_post) / Pmax_baseline",
            "acceptance": "≤ 5%"
          }
        }
      ]
    }
  },
