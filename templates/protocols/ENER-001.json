{
  "protocol_id": "ENER-001",
  "protocol_name": "Energy Rating Test",
  "version": "1.0.0",
  "category": "performance",
  "standard_reference": "IEC 61853-3:2018",
  "description": "Energy rating calculation for different climate zones",
  "test_parameters": [
    {
      "name": "climate_zones",
      "type": "list",
      "default": [
        "tropical",
        "moderate",
        "desert"
      ]
    },
    {
      "name": "orientation",
      "type": "string",
      "default": "south_facing"
    }
  ],
  "data_fields": [
    {
      "name": "climate_type",
      "type": "string"
    },
    {
      "name": "energy_yield",
      "type": "float",
      "unit": "kWh/kWp"
    },
    {
      "name": "performance_ratio",
      "type": "float"
    }
  ],
  "analysis_methods": [
    "climate_specific_modeling",
    "loss_analysis",
    "pr_calculation"
  ],
  "validation_criteria": [
    {
      "parameter": "energy_yield",
      "min": 800,
      "max": 2500
    }
  ],
  "visualization_types": [
    "climate_comparison",
    "monthly_profiles",
    "loss_breakdown"
  ],
