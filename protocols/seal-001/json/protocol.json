{
  "version": "1.0.0",
  "protocol_id": "SEAL-001",
  "name": "Edge Seal Degradation Assessment",
  "description": "Evaluation of edge seal integrity and moisture ingress resistance. Tracks delamination, discoloration, and barrier properties.",
  "type": "reliability",
  "metadata": {
    "author": "PV Test Lab",
    "created_date": "2025-11-13T00:00:00Z",
    "standard_reference": ["IEC 61215-2 MQT12", "ASTM E96"],
    "equipment_required": ["Moisture Analyzer", "Microscope", "Peel Test Equipment", "Environmental Chamber"],
    "estimated_duration_minutes": 240
  },
  "parameters": {
    "sample_id": {"type": "string", "required": true},
    "inspection_points": {"type": "integer", "default": 12, "min": 4, "max": 24},
    "exposure_duration": {"type": "number", "unit": "hours", "default": 1000, "min": 100, "max": 5000},
    "moisture_conditions": {"type": "select", "enum": ["85C/85RH", "60C/95RH", "ambient"], "default": "85C/85RH"}
  },
  "data_collection": {
    "channels": [
      {"channel_id": "DELAMINATION_LENGTH", "name": "Delamination Length", "unit": "mm", "data_type": "numeric", "storage_precision": 1},
      {"channel_id": "MOISTURE_INGRESS", "name": "Moisture Ingress Distance", "unit": "mm", "data_type": "numeric", "storage_precision": 1},
      {"channel_id": "PEEL_STRENGTH", "name": "Peel Strength", "unit": "N/mm", "data_type": "numeric", "storage_precision": 2},
      {"channel_id": "DISCOLORATION", "name": "Edge Discoloration", "unit": "mm", "data_type": "numeric", "storage_precision": 1}
    ]
  },
  "qc_criteria": {
    "tests": [
      {"test_id": "QC_001", "name": "Delamination Limit", "condition": "max(DELAMINATION_LENGTH) <= 5.0", "severity": "pass_fail"},
      {"test_id": "QC_002", "name": "Moisture Ingress Limit", "condition": "max(MOISTURE_INGRESS) <= 3.0", "severity": "warning"}
    ]
  },
  "analysis": {
    "calculations": [
      {"calc_id": "CALC_001", "name": "Mean Delamination", "formula": "mean(DELAMINATION_LENGTH)", "unit": "mm"},
      {"calc_id": "CALC_002", "name": "Max Ingress", "formula": "max(MOISTURE_INGRESS)", "unit": "mm"},
      {"calc_id": "CALC_003", "name": "Peel Strength Retention", "formula": "current_peel / baseline_peel * 100", "unit": "%"}
    ]
  }
}
