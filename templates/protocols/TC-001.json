{
  "protocol_id": "TC-001",
  "protocol_name": "Thermal Cycling Test",
  "version": "1.0.0",
  "category": "environmental",
  "standard_reference": "IEC 61215-2:2021 MQT 11",
  "description": "Assessment of module durability under thermal stress cycles",
  "test_parameters": [
    {
      "name": "number_of_cycles",
      "type": "int",
      "default": 200
    },
    {
      "name": "temp_min",
      "type": "float",
      "default": -40
    },
    {
      "name": "temp_max",
      "type": "float",
      "default": 85
    },
    {
      "name": "dwell_time",
      "type": "float",
      "unit": "minutes",
      "default": 10
    }
  ],
  "data_fields": [
    {
      "name": "cycle_number",
      "type": "array"
    },
    {
      "name": "power_output",
      "type": "array"
    },
    {
      "name": "visual_inspection",
      "type": "dict"
    },
    {
      "name": "el_imaging",
      "type": "image"
    }
  ],
  "analysis_methods": [
    "degradation_analysis",
    "failure_mode_identification"
  ],
  "validation_criteria": [
    {
      "parameter": "power_loss",
      "max": 5.0
    },
    {
      "parameter": "no_major_defects",
      "value": true
    }
  ],
  "visualization_types": [
    "cycle_degradation_curve",
    "thermal_profile",
    "defect_progression"
