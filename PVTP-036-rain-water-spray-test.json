{
  "protocol_metadata": {
    "protocol_id": "PVTP-036",
    "version": "1.0",
    "title": "Rain & Water Spray Test",
    "description": "Water ingress protection test for PV modules simulating heavy rain, driving rain, and water spray exposure",
    "test_type": "environmental_water_ingress",
    "created_date": "2025-11-12",
    "last_modified": "2025-11-12",
    "author": "PV Testing Laboratory",
    "status": "active",
    "compliance_standards": [
      {
        "standard_id": "IEC 61215",
        "version": "4.0",
        "section": "MQT 12",
        "title": "Outdoor Exposure Test (includes weather exposure)"
      },
      {
        "standard_id": "IEC 60529",
        "version": "2020",
        "section": "IPX4 / IPX5",
        "title": "Water spray protection"
      },
      {
        "standard_id": "IEC 61730",
        "version": "2.0",
        "section": "MST 52",
        "title": "Environmental Testing - Rain"
      },
      {
        "standard_id": "ASTM D5894",
        "version": "2016",
        "title": "Standard Practice for Cyclic Salt Fog/UV Exposure of Painted Metal (includes water spray)"
      }
    ],
    "related_protocols": ["PVTP-033", "PVTP-035", "PVTP-039"],
    "approval_status": {
      "reviewed_by": "Environmental Test Engineer",
      "approved_by": "Quality Director",
      "approval_date": "2025-11-12"
    }
  },
  "test_objectives": {
    "primary_objective": "Verify PV module water ingress protection under simulated rain and water spray conditions",
    "secondary_objectives": [
      "Assess junction box and cable gland water sealing effectiveness",
      "Evaluate frame drainage system performance",
      "Verify edge sealing integrity under water exposure",
      "Measure electrical safety after water exposure (wet leakage current)",
      "Validate suitability for outdoor installation in all climates"
    ],
    "acceptance_criteria": {
      "visual_inspection": "No water ingress into junction box or laminate",
      "electrical_performance": "Power degradation ≤ 2%",
      "insulation_resistance_wet": "≥ 40 MΩ after water spray",
      "wet_leakage_current": "Pass per IEC 61215",
      "safety": "No electrical shock hazard during wet conditions"
    },
    "test_scenarios": [
      {
        "scenario": "Vertical Rain (IPX3)",
        "description": "Simulates normal rainfall on module face",
        "spray_angle": "60° from vertical",
        "duration": "10 minutes per test cycle"
      },
      {
        "scenario": "Driving Rain (IPX4)",
        "description": "Simulates wind-driven rain from all directions",
        "spray_angle": "All directions",
        "duration": "10 minutes per direction (40 min total)"
      },
      {
        "scenario": "High-Pressure Water Jet (IPX5)",
        "description": "Simulates extreme storm conditions and cleaning",
        "water_pressure": "30 kPa at 3m distance",
        "duration": "3 minutes per test area"
      }
    ]
  },
  "test_equipment": {
    "rain_spray_chamber": {
      "manufacturer": "Weiss Umwelttechnik",
      "model": "WK3-600 Rain Test Chamber",
      "serial_number": "To be recorded",
      "specifications": {
        "chamber_volume": {
          "value": 600,
          "unit": "liters"
        },
        "spray_system": "Oscillating spray nozzles with adjustable flow",
        "water_flow_rate": {
          "ipx3": "10 L/min",
          "ipx4": "10 L/min",
          "ipx5": "12.5 L/min at 30 kPa"
        },
        "spray_angle_control": "Adjustable 0-90° from vertical",
        "test_module_mounting": "Rotating platform for all-angle exposure",
        "water_collection_system": "Drain with flow measurement"
      },
      "calibration": {
        "frequency": "Annual",
        "last_calibration_date": "2025-08-01",
        "next_calibration_date": "2026-08-01",
        "calibration_certificate": "CAL-RAIN-2025-001",
        "calibrated_parameters": ["Flow rate", "Spray pattern", "Pressure"]
      }
    },
    "water_quality_specification": {
      "type": "Deionized or distilled water",
      "conductivity": "< 5 μS/cm at 25°C",
      "ph": "6.5-7.5",
      "temperature": "15-30°C",
      "note": "Clean water prevents false failures from contamination"
    },
    "measurement_instruments": {
      "flow_meter": {
        "type": "Magnetic flow meter",
        "range": "0-50 L/min",
        "accuracy": "±2%"
      },
      "pressure_gauge": {
        "range": "0-100 kPa",
        "accuracy": "±1 kPa"
      },
      "iv_curve_tracer": {
        "model": "PVE PV Analyzer 1500V",
        "accuracy": "±0.5%",
        "purpose": "Pre/post electrical characterization"
      },
      "insulation_tester": {
        "model": "Fluke 1550C",
        "range": "250V - 5kV",
        "purpose": "Wet insulation resistance measurement"
      },
      "leakage_current_meter": {
        "model": "Chroma 19032",
        "range": "0.1 μA - 10 mA",
        "accuracy": "±1%",
        "purpose": "Wet leakage current test per IEC 61215"
      },
      "moisture_meter": {
        "model": "Tramex MRH III",
        "purpose": "Detect moisture in junction box"
      },
      "thermohygrometer": {
        "model": "Vaisala HMT330",
        "temperature_accuracy": "±0.2°C",
        "humidity_accuracy": "±1.0% RH"
      }
    }
  },
  "test_sequence": {
    "pre_test_phase": {
      "duration": "4-8 hours",
      "steps": [
        {
          "step_number": 1,
          "action": "Visual inspection and documentation",
          "parameters": {
            "inspection_areas": [
              "Junction box lid and gasket",
              "Cable glands and conduit entries",
              "Frame drainage holes",
              "Edge sealing (perimeter)",
              "Back sheet integrity",
              "Module connectors (if present)"
            ],
            "photography": "High-resolution images of all sealing points"
          },
          "acceptance": "All seals intact, no pre-existing water damage"
        },
        {
          "step_number": 2,
          "action": "Electrical characterization (baseline dry conditions)",
          "parameters": {
            "measurements": ["Voc", "Isc", "Pmax", "Vmp", "Imp", "FF"],
            "irradiance": "1000 W/m²",
            "module_temperature": "25°C ±2°C",
            "spectrum": "AM 1.5G"
          },
          "data_logging": {
            "file_name": "baseline_dry_iv_curve.csv",
            "parameters_logged": ["voltage", "current", "power", "temperature", "irradiance"]
          }
        },
        {
          "step_number": 3,
          "action": "Insulation resistance test (dry baseline)",
          "parameters": {
            "test_voltage": "1000V DC",
            "duration": "60 seconds",
            "min_resistance": "40 MΩ"
          }
        },
        {
          "step_number": 4,
          "action": "Module cleaning",
          "parameters": {
            "cleaning_method": "Deionized water and soft cloth",
            "drying": "Air dry minimum 2 hours"
          }
        }
      ]
    },
    "water_spray_test_phase": {
      "duration": "2-3 hours (multiple test scenarios)",
      "test_scenarios": [
        {
          "scenario_id": "IPX3_vertical_rain",
          "description": "Vertical rain spray test",
          "steps": [
            {
              "step_number": 5,
              "action": "Module installation for IPX3 test",
              "parameters": {
                "mounting_angle": "Module tilted at typical installation angle (20-30°)",
                "orientation": "Front glass facing spray",
                "spray_configuration": "Oscillating spray ±60° from vertical"
              }
            },
            {
              "step_number": 6,
              "action": "Execute IPX3 spray (10 minutes)",
              "parameters": {
                "water_flow_rate": "10 L/min",
                "spray_angle": "±60° from vertical",
                "duration": "10 minutes",
                "module_orientation": "Front surface exposed"
              },
              "data_logging": {
                "parameters": ["timestamp", "flow_rate", "water_temperature", "spray_angle"],
                "frequency": "Every 30 seconds"
              }
            }
          ]
        },
        {
          "scenario_id": "IPX4_driving_rain",
          "description": "All-direction water spray (driving rain)",
          "steps": [
            {
              "step_number": 7,
              "action": "Execute IPX4 spray from 4 directions",
              "parameters": {
                "water_flow_rate": "10 L/min",
                "spray_angle": "All directions (0°, 90°, 180°, 270°)",
                "duration_per_direction": "10 minutes",
                "total_duration": "40 minutes"
              },
              "data_logging": {
                "parameters": ["timestamp", "flow_rate", "spray_direction", "elapsed_time"],
                "frequency": "Every 30 seconds"
              }
            }
          ]
        },
        {
          "scenario_id": "IPX5_high_pressure_jet",
          "description": "High-pressure water jet (storm simulation)",
          "steps": [
            {
              "step_number": 8,
              "action": "Execute IPX5 high-pressure spray",
              "parameters": {
                "water_flow_rate": "12.5 L/min",
                "water_pressure": "30 kPa (at 3m distance)",
                "nozzle_diameter": "6.3 mm",
                "spray_pattern": "Direct jet at critical sealing areas",
                "target_areas": [
                  "Junction box lid seam",
                  "Cable gland entries",
                  "Frame joints",
                  "Module edges"
                ],
                "duration_per_area": "3 minutes",
                "total_duration": "15 minutes"
              },
              "data_logging": {
                "parameters": ["timestamp", "flow_rate", "pressure", "target_area"],
                "frequency": "Every 30 seconds"
              }
            }
          ]
        }
      ]
    },
    "post_spray_inspection_phase": {
      "duration": "2-4 hours",
      "steps": [
        {
          "step_number": 9,
          "action": "Immediate post-spray visual inspection (wet condition)",
          "parameters": {
            "timing": "Within 5 minutes of spray completion",
            "inspection": [
              "Check for water pooling on module surface",
              "Verify drainage from frame weep holes",
              "Look for water seepage at seals",
              "Photograph any anomalies"
            ]
          }
        },
        {
          "step_number": 10,
          "action": "Wet leakage current test (IEC 61215 Section 10.15)",
          "parameters": {
            "timing": "Immediately after spray, module still wet",
            "test_voltage": "1.25 × (Voc_max + 1000V) or as specified in IEC 61215",
            "surface_resistance": "Must be < 1 MΩ (verify surface is wet)",
            "leakage_current_limit": "< 1 mA per module",
            "duration": "60 seconds",
            "safety": "High voltage - follow LOTO, use insulated test fixture"
          }
        },
        {
          "step_number": 11,
          "action": "Wet insulation resistance test",
          "parameters": {
            "timing": "Immediately after wet leakage current test",
            "test_voltage": "1000V DC",
            "duration": "60 seconds",
            "min_resistance": "40 MΩ"
          }
        },
        {
          "step_number": 12,
          "action": "Junction box internal inspection",
          "parameters": {
            "timing": "Open junction box within 10 minutes of spray completion",
            "inspection": [
              "Visual inspection for water droplets or moisture",
              "Moisture meter measurement inside junction box",
              "Check terminals and diodes for water contact",
              "Photograph interior condition"
            ],
            "acceptance": "No water ingress (dry interior)"
          }
        },
        {
          "step_number": 13,
          "action": "Frame drainage and edge seal inspection",
          "parameters": {
            "drainage_check": "Verify water drained from frame, no pooling",
            "edge_inspection": "Check for water ingress at module perimeter seal",
            "delamination_check": "Look for edge delamination or bubbling (early indicator)"
          }
        },
        {
          "step_number": 14,
          "action": "Module drying",
          "parameters": {
            "method": "Air dry at room temperature",
            "duration": "Minimum 24 hours",
            "environment": "20°C ±5°C, < 70% RH"
          }
        },
        {
          "step_number": 15,
          "action": "Electrical characterization (post-test dry)",
          "parameters": {
            "timing": "After 24 hour drying period",
            "measurements": ["Voc", "Isc", "Pmax", "Vmp", "Imp", "FF"],
            "irradiance": "1000 W/m²",
            "module_temperature": "25°C ±2°C",
            "spectrum": "AM 1.5G"
          },
          "data_logging": {
            "file_name": "post_test_dry_iv_curve.csv",
            "parameters_logged": ["voltage", "current", "power", "temperature", "irradiance"]
          }
        },
        {
          "step_number": 16,
          "action": "Insulation resistance test (post-test dry)",
          "parameters": {
            "test_voltage": "1000V DC",
            "duration": "60 seconds",
            "min_resistance": "40 MΩ"
          }
        },
        {
          "step_number": 17,
          "action": "Pass/fail determination",
          "parameters": {
            "primary_criteria": [
              "No water ingress into junction box",
              "Pass wet leakage current test",
              "Wet insulation resistance ≥ 40 MΩ"
            ],
            "secondary_criteria": [
              "Power degradation ≤ 2%",
              "No edge delamination or seal damage",
              "Frame drainage functioning properly"
            ]
          }
        }
      ]
    }
  },
