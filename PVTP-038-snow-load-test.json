          }
        ],
        "visualization": [
          "Pre/post IV curve overlay",
          "Power degradation bar chart"
        ]
      },
      "dimensional_comparison": {
        "metrics": [
          "Permanent deformation (bow/twist)",
          "Frame dimensional changes"
        ],
        "acceptance": "< 1% of module dimension"
      }
    },
    "degradation_modeling": {
      "snow_load_field_correlation": {
        "approach": "Test load represents worst-case snow accumulation",
