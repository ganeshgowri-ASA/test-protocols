  "test_sequence": {
    "pre_test_phase": {
      "duration": "4-8 hours",
      "steps": [
        {
          "step_number": 1,
          "action": "Visual inspection and documentation",
          "parameters": {
            "inspection_areas": [
              "Front glass (busbar visibility and color)",
              "Junction box and terminals",
              "Frame and fasteners",
              "Cable glands and seals",
              "Encapsulation edges",
              "Back sheet"
            ],
            "color_measurement": "CIE L*a*b* measurement of silver busbars (baseline)",
            "photography": "High-resolution images (macro of busbars, junction box)"
          },
          "acceptance": "No pre-existing corrosion or discoloration"
        },
        {
          "step_number": 2,
          "action": "Electrical characterization (baseline)",
          "parameters": {
            "measurements": ["Voc", "Isc", "Pmax", "Vmp", "Imp", "FF", "Rs", "Rsh"],
            "irradiance": "1000 W/m²",
            "module_temperature": "25°C ±2°C",
            "spectrum": "AM 1.5G"
          },
          "data_logging": {
            "file_name": "baseline_iv_curve.csv",
            "parameters_logged": ["voltage", "current", "power", "temperature", "irradiance"]
          }
        },
        {
          "step_number": 3,
          "action": "Insulation resistance test (initial)",
          "parameters": {
            "test_voltage": "1000V DC",
            "duration": "60 seconds",
            "min_resistance": "40 MΩ"
          }
        },
        {
          "step_number": 4,
          "action": "Dielectric withstand test (initial)",
          "parameters": {
            "test_voltage": "1000V + 2×Voc_max",
            "duration": "60 seconds",
            "acceptance": "No breakdown"
          }
        },
        {
          "step_number": 5,
          "action": "Module cleaning",
          "parameters": {
            "cleaning_method": "Soft cloth with deionized water",
            "drying": "Air dry minimum 4 hours",
            "handling": "Clean gloves to prevent contamination"
          }
        },
        {
          "step_number": 6,
          "action": "Chamber preparation and safety verification",
          "parameters": {
            "chamber_cleaning": "Purge residual ammonia, clean interior",
            "safety_systems_check": "Gas detection, exhaust scrubber, emergency purge",
            "ammonia_analyzer_calibration": "Zero and span calibration",
            "personal_safety": "PPE available, exposure monitoring active"
          }
        }
      ]
    },
    "exposure_test_phase": {
      "duration": "1000 hours (41.67 days)",
      "test_conditions": {
        "ammonia_concentration": {
          "target": 500,
          "unit": "ppm",
          "tolerance": "±50 ppm",
          "monitoring": "Continuous with NH3 analyzer"
        },
        "temperature": {
          "target": 85,
          "unit": "°C",
          "tolerance": "±2°C",
          "monitoring": "Continuous"
        },
        "relative_humidity": {
          "target": 85,
          "unit": "%",
          "tolerance": "±5%",
          "monitoring": "Continuous"
        },
        "test_mode": "Continuous exposure (no cycling)"
      },
      "steps": [
        {
          "step_number": 7,
          "action": "Module installation in chamber",
          "parameters": {
            "mounting": "Open-rack support, no enclosure",
            "orientation": "Front glass facing up (45° tilt)",
            "spacing": "Minimum 150 mm between modules for gas circulation",
            "electrical_connection": "Cables and junction box fully exposed to ammonia atmosphere"
          }
        },
        {
          "step_number": 8,
          "action": "Chamber conditioning phase",
          "parameters": {
            "sequence": [
              "1. Heat chamber to 85°C without ammonia (2 hours)",
              "2. Stabilize humidity at 85% RH (2 hours)",
              "3. Introduce ammonia gradually to 500 ppm (1 hour)",
              "4. Verify stable conditions (1 hour)"
            ],
            "verification": "All parameters within tolerance before starting 1000h timer"
          }
        },
        {
          "step_number": 9,
          "action": "Continuous ammonia exposure (1000 hours)",
          "parameters": {
            "duration": "1000 hours continuous",
            "monitoring_frequency": "Every 5 minutes",
            "operator_checks": "Daily visual check through chamber window (no opening)"
          },
          "data_logging": {
            "parameters": [
              "timestamp",
              "elapsed_time_hours",
              "chamber_temperature",
              "chamber_humidity",
              "ammonia_concentration",
              "chamber_pressure"
            ],
            "sampling_rate": "5 minutes",
            "file_format": "CSV"
          }
        },
        {
          "step_number": 10,
          "action": "Interim inspections (at 250h, 500h, 750h)",
          "parameters": {
            "inspection_method": "Visual through chamber observation port (no opening)",
            "observation_targets": [
              "Visible discoloration of silver busbars",
              "Junction box condition",
              "Any signs of delamination or bubbling"
            ],
            "photography": "Through chamber window if possible",
            "acceptance": "Test continues regardless of observations (unless safety concern)"
          }
        }
      ]
    },
    "post_test_phase": {
      "duration": "2-3 days",
      "steps": [
        {
          "step_number": 11,
          "action": "Chamber purge and cooldown",
          "parameters": {
            "sequence": [
              "1. Stop ammonia injection",
              "2. Purge with fresh air for 2 hours (ammonia scrubber active)",
              "3. Reduce temperature to 30°C",
              "4. Verify ammonia concentration < 5 ppm before opening",
              "5. PPE required for chamber access"
            ],
            "safety_verification": "Gas detection confirms safe NH3 level"
          }
        },
        {
          "step_number": 12,
          "action": "Module removal and initial inspection",
          "parameters": {
            "ppe_required": "Respirator, chemical gloves, safety glasses",
            "handling": "Careful removal, avoid touching corroded surfaces",
            "initial_photography": "Document as-removed condition (before cleaning)"
          }
        },
        {
          "step_number": 13,
          "action": "Visual inspection and corrosion assessment",
          "parameters": {
            "inspection_areas": [
              "Silver busbar discoloration and corrosion",
              "Junction box terminal corrosion",
              "Frame and fastener condition",
              "Encapsulation edge sealing",
              "Back sheet condition",
              "Cable insulation integrity"
            ],
            "color_measurement": "CIE L*a*b* measurement of busbars (post-test)",
            "color_change_calculation": "ΔE = √[(ΔL*)² + (Δa*)² + (Δb*)²]",
            "photography": "High-resolution macro images matching pre-test angles"
          }
        },
        {
          "step_number": 14,
          "action": "Module cleaning procedure",
          "parameters": {
            "first_rinse": "Deionized water spray to remove ammonia residue",
            "rinse_duration": "5 minutes",
            "neutralization": "Optional: dilute acetic acid rinse for heavy ammonia deposits",
            "final_rinse": "Deionized water spray",
            "drying": "Air dry at 20°C for 24 hours minimum"
          }
        },
        {
          "step_number": 15,
          "action": "Electrical characterization (post-test)",
          "parameters": {
            "wait_time": "Minimum 24 hours after cleaning and drying",
            "measurements": ["Voc", "Isc", "Pmax", "Vmp", "Imp", "FF", "Rs", "Rsh"],
            "irradiance": "1000 W/m²",
            "module_temperature": "25°C ±2°C",
            "spectrum": "AM 1.5G"
          },
          "data_logging": {
            "file_name": "post_test_iv_curve.csv",
            "parameters_logged": ["voltage", "current", "power", "temperature", "irradiance"]
          }
        },
        {
          "step_number": 16,
          "action": "Insulation resistance test (post-test)",
          "parameters": {
            "test_voltage": "1000V DC",
            "duration": "60 seconds",
            "min_resistance": "40 MΩ"
          }
        },
        {
          "step_number": 17,
          "action": "Dielectric withstand test (post-test)",
          "parameters": {
            "test_voltage": "1000V + 2×Voc_max",
            "duration": "60 seconds",
            "acceptance": "No breakdown"
          }
        },
        {
          "step_number": 18,
          "action": "Performance degradation and pass/fail determination",
          "parameters": {
            "degradation_calculation": "Δ% = 100 × (Pmax_pre - Pmax_post) / Pmax_pre",
            "visual_acceptance": "No failure of critical components",
            "combined_assessment": "Electrical + visual + insulation results"
          }
        }
      ]
    }
  },
