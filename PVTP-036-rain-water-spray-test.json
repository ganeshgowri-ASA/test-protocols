  "analysis_and_reporting": {
    "time_series_analysis": {
      "spray_consistency_assessment": {
        "metrics": [
          "Flow rate stability over test duration",
          "Pressure consistency (IPX5)",
          "Water temperature variation"
        ],
        "visualization": [
          "Flow rate vs. time",
          "Pressure control chart (IPX5)"
        ]
      }
    },
    "baseline_comparison": {
      "electrical_parameters": {
        "comparison_metrics": [
          {
            "parameter": "Pmax",
            "calculation": "Δ% = 100 × (Pmax_post_dry - Pmax_baseline_dry) / Pmax_baseline_dry",
