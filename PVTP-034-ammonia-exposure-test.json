{
  "protocol_metadata": {
    "protocol_id": "PVTP-034",
    "version": "1.0",
    "title": "Ammonia Exposure Test (IEC 62716)",
    "description": "Accelerated corrosion test for PV modules exposed to ammonia-rich environments (agricultural, livestock facilities)",
    "test_type": "environmental_chemical_corrosion",
    "created_date": "2025-11-12",
    "last_modified": "2025-11-12",
    "author": "PV Testing Laboratory",
    "status": "active",
    "compliance_standards": [
      {
        "standard_id": "IEC 62716",
        "version": "2013",
        "edition": "1.0",
        "title": "Photovoltaic (PV) modules - Ammonia corrosion testing",
        "requirements": [
          "500 ppm NH3 concentration",
          "85°C chamber temperature",
          "85% relative humidity",
          "1000 hours total exposure",
          "Power degradation ≤ 5%, no critical corrosion"
        ]
      },
      {
        "standard_id": "IEC 61215",
        "version": "4.0",
        "section": "MQT 22",
        "title": "Ammonia Corrosion Test (optional)"
      }
    ],
    "related_protocols": ["PVTP-033", "PVTP-039"],
    "approval_status": {
      "reviewed_by": "Chemical Testing Specialist",
      "approved_by": "Quality Director",
      "approval_date": "2025-11-12"
    }
  },
  "test_objectives": {
    "primary_objective": "Evaluate PV module resistance to ammonia-induced corrosion per IEC 62716",
    "secondary_objectives": [
      "Assess corrosion resistance of metallization (silver busbars)",
      "Evaluate junction box and interconnect resistance to ammonia",
      "Measure electrical performance degradation after exposure",
      "Identify ammonia-sensitive materials and components",
      "Validate suitability for agricultural/livestock facility installations"
    ],
    "acceptance_criteria": {
      "visual_inspection": "No major visual defects (silver corrosion, encapsulation failure)",
      "electrical_performance": "Power degradation ≤ 5% from baseline",
      "insulation_resistance": "≥ 40 MΩ post-test",
      "safety": "Pass dielectric withstand test",
      "corrosion_severity": "No failure of critical components (busbars, interconnects)"
    },
    "application_context": {
      "target_environments": [
        "Agricultural installations near livestock facilities",
        "Poultry and dairy farm rooftops",
        "Fertilizer storage areas",
        "Wastewater treatment facilities"
      ],
      "field_ammonia_concentrations": "Typically 10-50 ppm, peaks to 100+ ppm near sources",
      "accelerated_test_factor": "500 ppm for 1000h ≈ 5-10 years field exposure"
    }
  },
