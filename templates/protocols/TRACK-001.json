{
  "protocol_id": "TRACK-001",
  "protocol_name": "Tracking System Performance Evaluation",
  "version": "1.0.0",
  "category": "performance",
  "standard_reference": "IEC 62817:2014",
  "description": "Performance evaluation of solar tracking systems",
  "test_parameters": [
    {
      "name": "tracking_type",
      "type": "string",
      "default": "single_axis"
    },
    {
      "name": "tracking_accuracy",
      "type": "float",
      "unit": "degrees"
    }
  ],
  "data_fields": [
    {
      "name": "time",
      "type": "datetime"
    },
    {
      "name": "sun_position",
      "type": "dict"
    },
    {
      "name": "tracker_position",
      "type": "dict"
    },
    {
      "name": "power_output",
      "type": "float"
    }
  ],
  "analysis_methods": [
    "tracking_gain_calculation",
    "accuracy_analysis",
    "energy_comparison"
  ],
  "validation_criteria": [
    {
      "parameter": "tracking_accuracy",
      "max": 5.0
    },
    {
      "parameter": "tracking_gain",
      "min": 1.2
    }
  ],
