{
  "protocol_metadata": {
    "protocol_id": "PVTP-035",
    "version": "1.0",
    "title": "Sand & Dust Ingress Test (IEC 60529 IP6X)",
    "description": "Dust tightness verification test for PV modules to assess protection against fine dust ingress in desert and arid environments",
    "test_type": "environmental_ingress_protection",
    "created_date": "2025-11-12",
    "last_modified": "2025-11-12",
    "author": "PV Testing Laboratory",
    "status": "active",
    "compliance_standards": [
      {
        "standard_id": "IEC 60529",
        "version": "2020",
        "edition": "3.0",
        "section": "IP6X",
        "title": "Degrees of protection provided by enclosures (IP Code) - Dust tight",
        "requirements": [
          "No ingress of dust after 8 hours exposure at 20 Pa underpressure",
          "Talc powder test dust (particle size < 75 μm)",
          "Test temperature 40°C ±5°C",
          "Visual inspection: no dust deposit inside enclosure"
        ]
      },
      {
        "standard_id": "IEC 61215",
        "version": "4.0",
        "section": "10.18",
        "title": "Bypass Diode Thermal Test (includes dust ingress check for junction box)"
      },
      {
        "standard_id": "IEC 61730",
        "version": "2.0",
        "section": "MST 51",
        "title": "Environmental Testing - Dust"
      }
    ],
    "related_protocols": ["PVTP-033", "PVTP-036", "PVTP-039"],
    "approval_status": {
      "reviewed_by": "Environmental Test Engineer",
      "approved_by": "Quality Director",
      "approval_date": "2025-11-12"
    }
  },
  "test_objectives": {
    "primary_objective": "Verify PV module dust-tight protection per IEC 60529 IP6X rating",
    "secondary_objectives": [
      "Assess junction box sealing integrity against fine dust ingress",
      "Evaluate cable gland/conduit entry seal effectiveness",
      "Verify frame drainage holes do not allow dust ingress to electrical compartments",
      "Assess gasket and seal performance under simulated desert conditions",
      "Validate module suitability for desert and arid climate installations"
    ],
    "acceptance_criteria": {
      "visual_inspection_internal": "No visible dust deposits inside junction box or electrical compartments",
      "electrical_performance": "No degradation in insulation resistance or electrical parameters",
      "seal_integrity": "All seals and gaskets intact, no visible damage",
      "safety": "Pass insulation resistance test post-exposure"
    },
    "application_context": {
      "target_environments": [
        "Desert PV installations",
        "Middle East and North Africa (MENA) region",
        "Dry, arid climates with high dust concentrations",
        "Mining areas",
        "Agricultural fields with soil dust"
      ],
      "field_dust_challenges": "Fine particulate ingress can cause electrical tracking, corrosion, thermal issues"
    }
  },
  "test_equipment": {
    "dust_test_chamber": {
      "manufacturer": "Weisstechnik or equivalent",
      "model": "Dust Chamber IP6X Certified",
      "serial_number": "To be recorded",
      "specifications": {
        "chamber_volume": {
          "value": 1000,
          "unit": "liters"
        },
        "test_dust_type": "Talc powder per IEC 60529 Annex C",
        "dust_particle_size": "< 75 μm",
        "dust_concentration": {
          "value": 2,
          "unit": "kg/m³",
          "note": "Typical concentration in chamber"
        },
        "underpressure": {
          "value": 20,
          "unit": "Pa",
          "tolerance": "±2 Pa",
          "measurement": "Differential pressure transducer"
        },
        "temperature_control": {
          "range": "15-50°C",
          "accuracy": "±2°C"
        },
        "circulation_system": "Forced air circulation to maintain dust suspension",
        "exhaust_system": "HEPA filtered exhaust"
      },
      "calibration": {
        "frequency": "Annual",
        "last_calibration_date": "2025-07-01",
        "next_calibration_date": "2026-07-01",
        "calibration_certificate": "CAL-DUST-2025-001",
        "calibrated_parameters": ["Pressure control", "Temperature uniformity", "Air flow"]
      }
    },
    "test_dust_specification": {
      "material": "Talc powder (hydrated magnesium silicate)",
      "specification": "Per IEC 60529 Annex C",
      "particle_size_distribution": {
        "d50": "< 20 μm",
        "d90": "< 75 μm",
        "analysis_method": "Laser diffraction particle size analyzer"
      },
      "supplier": "NIST-traceable test dust supplier",
      "batch_certification": "Required with particle size analysis"
    },
    "measurement_instruments": {
      "differential_pressure_transducer": {
        "manufacturer": "Setra or Omega",
        "range": "0-100 Pa",
        "accuracy": "±0.5 Pa or ±1% of reading",
        "purpose": "Monitor chamber underpressure"
      },
      "particle_counter": {
        "model": "TSI AeroTrak 9000",
        "measurement_range": "0.3-25 μm",
        "purpose": "Verify dust concentration in chamber"
      },
      "iv_curve_tracer": {
        "model": "PVE PV Analyzer 1500V",
        "accuracy": "±0.5%",
        "purpose": "Pre/post electrical characterization"
      },
      "insulation_tester": {
        "model": "Fluke 1550C",
        "range": "250V - 5kV",
        "purpose": "Insulation resistance measurement"
      },
      "borescope": {
        "model": "Olympus IPLEX NX",
        "purpose": "Internal junction box inspection without disassembly"
      },
      "thermohygrometer": {
        "model": "Vaisala HMT330",
        "temperature_accuracy": "±0.2°C",
        "humidity_accuracy": "±1.0% RH"
      }
    }
  },
  "test_sequence": {
    "pre_test_phase": {
      "duration": "2-4 hours",
      "steps": [
        {
          "step_number": 1,
          "action": "Visual inspection and documentation",
          "parameters": {
            "inspection_areas": [
              "Junction box lid gasket condition",
              "Cable gland seals",
              "Frame drainage holes and weep holes",
              "Module perimeter seal",
              "Back sheet surface",
              "Frame joints"
            ],
            "photography": "High-resolution images of all sealing areas",
            "gasket_inspection": "Check for deformation, cracks, proper seating"
          },
          "acceptance": "All seals intact and properly seated"
        },
        {
          "step_number": 2,
          "action": "Electrical characterization (baseline)",
          "parameters": {
            "measurements": ["Voc", "Isc", "Pmax", "Vmp", "Imp", "FF"],
            "irradiance": "1000 W/m²",
            "module_temperature": "25°C ±2°C",
            "spectrum": "AM 1.5G"
          },
          "data_logging": {
            "file_name": "baseline_iv_curve.csv",
            "parameters_logged": ["voltage", "current", "power", "temperature", "irradiance"]
          }
        },
        {
          "step_number": 3,
          "action": "Insulation resistance test (initial)",
          "parameters": {
            "test_voltage": "1000V DC",
            "duration": "60 seconds",
            "min_resistance": "40 MΩ"
          }
        },
        {
          "step_number": 4,
          "action": "Junction box internal inspection (baseline)",
          "parameters": {
            "method": "Photograph junction box interior (if design allows non-destructive access) OR borescope inspection",
            "documentation": "Cleanliness verification, terminal condition",
            "note": "If junction box must remain sealed, skip this step and rely on post-test comparison"
          }
        },
        {
          "step_number": 5,
          "action": "Module cleaning",
          "parameters": {
            "cleaning_method": "Compressed air blow-off to remove any pre-existing dust",
            "inspection": "Verify all external surfaces clean",
            "handling": "Clean gloves to prevent contamination"
          }
        },
        {
          "step_number": 6,
          "action": "Chamber preparation and dust verification",
          "parameters": {
            "chamber_cleaning": "Remove residual dust from previous tests",
            "test_dust_preparation": "Weigh and load 2 kg/m³ of talc powder",
            "particle_size_verification": "Verify test dust batch certification",
            "pressure_system_check": "Verify underpressure control functional"
          }
        }
      ]
    },
    "dust_exposure_test_phase": {
      "duration": "8 hours",
      "test_conditions": {
        "exposure_duration": {
          "value": 8,
          "unit": "hours",
          "note": "Per IEC 60529 for IP6X rating"
        },
        "chamber_underpressure": {
          "value": 20,
          "unit": "Pa",
          "tolerance": "±2 Pa",
          "note": "Negative pressure inside module relative to chamber to drive dust ingress"
        },
        "chamber_temperature": {
          "value": 40,
          "unit": "°C",
          "tolerance": "±5°C"
        },
        "dust_concentration": {
          "value": 2,
          "unit": "kg/m³"
        },
        "air_circulation": "Forced circulation to maintain dust suspension"
      },
      "steps": [
        {
          "step_number": 7,
          "action": "Module installation in chamber",
          "parameters": {
            "mounting": "Module installed per intended field orientation",
            "orientation": "Junction box accessible to dust exposure",
            "underpressure_connection": "If applicable, internal module volume connected to vacuum source to create pressure differential",
            "note": "For junction box testing, ensure junction box interior is at lower pressure than chamber"
          }
        },
        {
          "step_number": 8,
          "action": "Chamber conditioning",
          "parameters": {
            "sequence": [
              "1. Close chamber, establish temperature at 40°C (1 hour)",
              "2. Introduce test dust to chamber",
              "3. Start air circulation to suspend dust",
              "4. Establish underpressure of 20 Pa (if applicable to module design)",
              "5. Verify stable conditions for 15 minutes"
            ],
            "verification": "Pressure and temperature within tolerance"
          }
        },
        {
          "step_number": 9,
          "action": "Execute 8-hour dust exposure",
          "parameters": {
            "duration": "8 hours continuous",
            "monitoring": "Pressure, temperature, dust suspension every 15 minutes",
            "operator_checks": "Visual check of chamber operation every 2 hours"
          },
          "data_logging": {
            "parameters": [
              "timestamp",
              "elapsed_time_hours",
              "chamber_temperature",
              "chamber_pressure",
              "underpressure_differential",
              "dust_concentration_estimate"
            ],
            "sampling_rate": "15 minutes",
            "file_format": "CSV"
          }
        },
        {
          "step_number": 10,
          "action": "Chamber shutdown and clearing",
          "parameters": {
            "sequence": [
              "1. Stop air circulation (dust settles)",
              "2. Release underpressure",
              "3. Wait 30 minutes for dust to settle",
              "4. Carefully open chamber to avoid dust disturbance"
            ],
            "safety": "Dust mask and safety glasses for chamber access"
          }
        }
      ]
    },
    "post_test_phase": {
      "duration": "2-4 hours",
      "steps": [
        {
          "step_number": 11,
          "action": "External cleaning (do not open junction box yet)",
          "parameters": {
            "cleaning_method": "Soft brush and compressed air to remove external dust",
            "areas_cleaned": "Frame, cable exterior, back sheet",
            "areas_NOT_cleaned": "Junction box interior (inspection first)",
            "photography": "Document as-removed condition before cleaning"
          }
        },
        {
          "step_number": 12,
          "action": "Junction box internal inspection (critical step)",
          "parameters": {
            "method": "Carefully open junction box lid OR use borescope if non-destructive inspection required",
            "inspection_criteria": [
              "Visual inspection for dust deposits on terminals, diodes, PCB",
              "Quantitative assessment: no visible dust (pass), minor dust traces (borderline), significant dust (fail)",
              "Photography: high-resolution macro images of interior"
            ],
            "acceptance_ip6x": "No dust ingress (dust tight)",
            "note": "This is the primary pass/fail criterion for IP6X"
          }
        },
        {
          "step_number": 13,
          "action": "Cable gland and seal inspection",
          "parameters": {
            "inspection": "Check for dust penetration around cable glands, conduit entries",
            "method": "Visual inspection, wipe test",
            "acceptance": "No dust ingress past seals"
          }
        },
        {
          "step_number": 14,
          "action": "Frame drainage hole inspection",
          "parameters": {
            "inspection": "Verify drainage holes did not allow dust into frame cavity or electrical compartments",
            "method": "Visual inspection, compressed air blow-through test",
            "acceptance": "No dust pathway to electrical components"
          }
        },
        {
          "step_number": 15,
          "action": "Complete external cleaning",
          "parameters": {
            "cleaning_method": "Soft cloth with deionized water, compressed air",
            "drying": "Air dry minimum 2 hours"
          }
        },
        {
          "step_number": 16,
          "action": "Electrical characterization (post-test)",
          "parameters": {
            "measurements": ["Voc", "Isc", "Pmax", "Vmp", "Imp", "FF"],
            "irradiance": "1000 W/m²",
            "module_temperature": "25°C ±2°C",
            "spectrum": "AM 1.5G"
          },
          "data_logging": {
            "file_name": "post_test_iv_curve.csv",
            "parameters_logged": ["voltage", "current", "power", "temperature", "irradiance"]
          }
        },
        {
          "step_number": 17,
          "action": "Insulation resistance test (post-test)",
          "parameters": {
            "test_voltage": "1000V DC",
            "duration": "60 seconds",
            "min_resistance": "40 MΩ"
          }
        },
        {
          "step_number": 18,
          "action": "Pass/fail determination",
          "parameters": {
            "primary_criterion": "No dust ingress into junction box or electrical compartments (IP6X)",
            "secondary_criteria": [
              "Insulation resistance ≥ 40 MΩ",
              "No electrical performance degradation",
              "Seals and gaskets intact"
            ],
            "overall_assessment": "Pass if all criteria met"
          }
        }
      ]
    }
  },
