        },
        {
          "parameter": "pressure_uniformity",
          "check_type": "coefficient_of_variation",
          "max_cv": "10%",
          "action_on_failure": "Check for air leaks"
        },
        {
          "parameter": "deflection_rate",
          "check_type": "threshold",
          "max_rate": "5 mm/hour (for sustained load - indicates accelerating creep)",
          "action_on_failure": "Warning - potential impending failure"
        }
      ]
    },
    "retention_policy": {
      "raw_mechanical_data": "10 years",
      "electrical_characterization": "Permanent",
      "photographs": "10 years",
      "test_reports": "Permanent"
    }
  },
  "analysis_and_reporting": {
    "time_series_analysis": {
      "deflection_behavior": {
        "metrics": [
          "Maximum deflection vs. applied load",
          "Elastic recovery (deflection returns to zero after load release)",
          "Creep behavior (deflection increase over time under constant load)"
        ],
        "visualization": [
          "Load vs. deflection curve",
          "Deflection vs. time (for sustained load test)",
          "Creep rate analysis (mm/hour vs. time)"
        ]
      },
      "creep_analysis": {
        "description": "For sustained load tests, assess time-dependent deformation",
        "primary_creep": "Initial rapid deflection increase (first minutes/hours)",
        "secondary_creep": "Steady-state deflection rate",
        "tertiary_creep": "Accelerating deflection (warning of imminent failure)",
        "acceptance": "No tertiary creep observed, secondary creep rate < 5 mm/hour"
      }
    },
    "baseline_comparison": {
      "electrical_parameters": {
        "comparison_metrics": [
          {
            "parameter": "Pmax",
            "calculation": "Δ% = 100 × (Pmax_post - Pmax_pre) / Pmax_pre",
